  "_moduleAliases": {
    "@assets": "dist/assets",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@modules": "dist/modules",
    "@middleware": "dist/middleware",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@validations": "dist/validations"
  }
